{
  "name": "sdcampus-website",
  "version": "2.2.8",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "prod": "npm run generate-meta-tag && npx env-cmd -f .env react-scripts start",
    "dev": "npm run generate-meta-tag && npx env-cmd -f .env.development react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts generate-meta-tag && GENERATE_SOURCEMAP=false react-scripts build",
    "generate-meta-tag": "node ./node_modules/react-cache-buster/dist/generate-meta-tag.js",
    "deploy": "aws s3 sync build/ s3://sdcampus.com",
    "sitemap": "babel-node ./src/sitemap-generator.js",
    "predeploy": "npm run sitemap",
    "deploy-prod": "npm run build && npm run deploy",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@cashfreepayments/cashfree-js": "^1.0.4",

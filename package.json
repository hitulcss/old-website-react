    "aos": "^2.3.4",
    "axios": "^1.6.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "dompurify": "^3.0.8",
    "firebase": "^10.12.2",
    "history": "^5.3.0",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",

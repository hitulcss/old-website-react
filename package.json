    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-lazy-load-image-component": "^1.6.4",
    "autoprefixer": "^10.4.17",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
